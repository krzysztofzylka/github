{
    "name": "krzysztozylka/github",
    "description": "PHP GitHub API client library",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Krzysztof Zylka",
            "email": "krzysztofzylka@yahoo.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "ext-curl": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "krzysztofzylka\\github\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "krzysztofzylka\\github\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}